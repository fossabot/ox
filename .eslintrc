{
  "parser": "babel-eslint",
  "plugins": ["babel"],
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "rules": {
    "no-console": "off",
    "implicit-arrow-linebreak": "off",
    "arrow-parens": [
      "error",
      "as-needed",
      {
        "requireForBlockBody": false
      }
    ],
    "object-curly-newline": "off",
    "function-paren-newline": "off"
  }
}
